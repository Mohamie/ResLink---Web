

<table class="table table-striped table-bordered table-dark">
    <tr>
        <th>Student Name</th>
        <td>@Complaint.Student.FullNames @Complaint.Student.LastName</td>
    </tr>

    <tr>
        <th>Student Room No</th>
        <td>@Complaint.Student.RoomNumber</td>
    </tr>

    <tr>
        <th>Student's PhoneNumber</th>
        <td>@Complaint.Student.PhoneNumber</td>
    </tr>

    <tr>
        <th>To Whom does Complaint Target</th>
        <td class="text-danger">Not Yet Implemented</td>
    </tr>

    <tr>
        <th>Complaint Status</th>
        <td class="text-danger">Not Yet Implemented</td>
    </tr>


    <tr>
        <th>Complaint Title</th>
        <td>@Complaint.ComplaintTitle</td>
    </tr>

    <tr>
        <th>Complaint Description</th>
        <td>@Complaint.ComplaintDescription</td>
    </tr>

</table>


@code {
    [Parameter]
    public Complaint Complaint { get; set; }

}
